<div id="home">
  <div class="container">
    <div class="row">
      <div class="col-8 offset-2">
        <h1>{{msg}}</h1>
        <h2>This is our Game Picker Page!</h2>

        <h3>Games:</h3>
        <ul id="game-list" class="list-group">
          {{#each games}}
          <li data-id="{{this.id}}" class="list-group-item">
            <a href="api/games/{{this.id}}">{{this.text}}</a>
            <!-- changed delete class to gamePick class -->
            <button class="btn btn-danger float-right gamePick">y</button>
          </li>
          {{/each}}
        </ul>
      </div>
    </div>
  </div>
</div>

<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
<script src="/js/gamePicker.js"></script>